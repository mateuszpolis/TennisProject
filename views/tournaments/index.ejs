<h2>SEARCH TOURNAMENTS (<%= numberOfTournaments %>)</h2>
<form action="/tournaments" method="GET">
  <label for="name">Name</label>
  <input type="text" name="name" value="<%= searchOptions.name%>" />
  <label for="location">Location</label>
  <input type="text" name="location" value="<%= searchOptions.location%>" />
  <label for="tournamentRank">Tournament Rank</label>
  <input
    type="number"
    name="tournamentRank"
    min="125"
    max="2000"
    step="5"
    value="<%= searchOptions.tournamentRank %>"
  />
  <input type="submit" value="Search" />
</form>
<br />
<% tournaments.forEach(tournament => { %>
<div>
  <%= tournament.name %> 
  <%= tournament.location %> 
  <%= tournament.tournamentRank %>
</div>
<% }) %>
